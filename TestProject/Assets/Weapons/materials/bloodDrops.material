
material Weapons/materials/bloodDrops.material
{
	program_shared_parameters
	{
		diffuseColor vec4 1.000000 1.000000 1.000000 1.000000
	}

	receive_shadows off
	technique Forward
	{
		pass perlight
		{
			lighting off
			scene_blend alpha_blend
			depth_write off
			cull_hardware none
			cull_software none

			preprocessor_defines
			{
			}

			texture_unit diffuseMap
			{
				texture_alias diffuseMap
				texture Weapons/textures/BloodDrops.psd
				max_anisotropy 8
			}
		}

	}

}
