
material StandardAssets/Skybox/Materials/DaySkybox.material
{
	program_shared_parameters
	{
		tintColor vec3 1.000000 1.000000 1.000000
	}

	technique Forward
	{
		pass Skybox
		{
			lighting off
			depth_write off

			preprocessor_defines
			{
			}

			vertex_program_ref Shaders/Skybox.glslv
			{
				param_named_auto worldViewProj worldviewproj_matrix
			}

			fragment_program_ref Shaders/Skybox.glslf
			{
				param_named cubemap int 0
				param_named tintColor float3 1 1 1
			}

			texture_unit cubemap
			{
				texture_alias cubemap
				cubemap_ref StandardAssets/Skybox/Cubemaps/Day.cubemap
				max_anisotropy 8
				tex_address_mode clamp
			}
		}

	}

}
