// Ambient_ps
fragment_program DeferredShading/post/Ambient_ps glsl
{
	source StandardAssets/DeferredShading/Shaders/Ambient_ps.glsl
	syntax glsl120
	
	default_params
	{
		param_named_auto ambientColor ambient_light_colour 0
		param_named_auto proj projection_matrix
		param_named_auto farClipDistance far_clip_distance
        param_named Tex0 int 0
        param_named Tex1 int 1
	}
}

// ShowNormal_ps
fragment_program DeferredShading/post/ShowNormal_ps glsl
{
	source StandardAssets/DeferredShading/Shaders/ShowNormal_ps.glsl
	syntax glsl120
	default_params
	{
        param_named Tex0 int 0
        param_named Tex1 int 1
    }
}

// ShowDS_ps
fragment_program DeferredShading/post/ShowDS_ps glsl
