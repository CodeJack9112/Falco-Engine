// Auxiliary lights
vertex_program DeferredShading/post/LightMaterial_vs glsl
{
	source StandardAssets/DeferredShading/Shaders/LightMaterial_vs.glsl
	syntax glsl120
	default_params
	{
		param_named_auto worldViewProj worldviewproj_matrix
	}
}

// Post processors
vertex_program DeferredShading/post/vs glsl
{
	source StandardAssets/DeferredShading/Shaders/vs.glsl
	syntax glsl120
	default_params
	{
		param_named_auto flip render_target_flipping
		param_named farCorner float3 1 1 1
	}
}